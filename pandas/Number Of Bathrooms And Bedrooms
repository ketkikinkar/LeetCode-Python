# Start writing code
airbnb_search_details.head()
result =(airbnb_search_details.groupby(['city','property_type'])['bathrooms','bedrooms'].mean().reset_index().rename
(columns= {'bathrooms':'n_bathrooms','bedrooms':'n_bedrooms'}))
